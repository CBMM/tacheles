{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/compatible": "^5.1.2",
        "antd": "^5.10.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^6.0.3",
        "react-scripts": "^5.0.1",
        "remark-gfm": "^1.0.0"
    },
    "scripts": {
        "start": "FRONTEND_MODE=dev react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "lint": "eslint \"src/**/*.js\"",
        "format": "prettier --write \"src/**/*.js\"",
        "test": "react-scripts test --testPathIgnorePatterns __tests__e2e__ --watchAll=false",
        "test:e2e": "react-scripts test --testPathPattern __tests__e2e__ --watchAll=false"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/preset-env": "^7.24.6",
        "@testing-library/jest-dom": "^6.4.5",
        "@testing-library/react": "^15.0.7",
        "@testing-library/user-event": "^14.5.2",
        "axios": "^1.7.2",
        "babel-jest": "^29.7.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.34.1",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "react-test-renderer": "^18.3.1",
        "text-encoding": "^0.7.0",
        "web-streams-polyfill": "^4.0.0"
    },
    "jest": {
        "moduleNameMapper": {
            "^axios$": "axios/dist/node/axios.cjs"
        }
    }
}
